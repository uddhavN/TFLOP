
{
  "dataset_name": "pubtabnet_cpu",
  "input_data": {
    "jsonl_path": "TFLOP-dataset/meta_data/dataset_train_1percent.jsonl",
    "images_dir": "TFLOP-dataset/images/train",
    "pse_results_dir": "TFLOP-dataset/pse_results/train"
  },
  "output_data": {
    "processed_dir": "TFLOP-dataset/processed_cpu",
    "cache_dir": null
  },
  "image_processing": {
    "target_size": [512, 512],
    "interpolation": "bilinear",
    "normalize": true,
    "mean": [0.485, 0.456, 0.406],
    "std": [0.229, 0.224, 0.225]
  },
  "text_processing": {
    "max_length": 512,
    "truncation": true,
    "padding": "max_length"
  },
  "memory_optimization": {
    "cache_images": false,
    "batch_size": 1,
    "num_workers": 2,
    "lazy_loading": true
  },
  "cpu_specific": {
    "enable_multiprocessing": false,
    "memory_limit_gb": 12,
    "cleanup_frequency": 10
  }
}
